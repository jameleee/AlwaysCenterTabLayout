<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="acl_CenterTabLayout">
    <attr name="acl_indicatorWithoutPadding" format="boolean"/>
    <attr name="acl_indicatorInFront" format="boolean"/>
    <attr name="acl_indicatorInterpolation" format="enum">
      <enum name="smart" value="0"/>
      <enum name="linear" value="1"/>
    </attr>
    <attr name="acl_indicatorGravity" format="enum">
      <enum name="bottom" value="0"/>
      <enum name="top" value="1"/>
      <enum name="center" value="2"/>
    </attr>
    <attr name="acl_indicatorColor" format="color"/>
    <attr name="acl_indicatorColors" format="reference"/>
    <attr name="acl_indicatorThickness" format="dimension"/>
    <attr name="acl_indicatorWidth" format="dimension">
      <enum name="auto" value="-1"/>
    </attr>
    <attr name="acl_indicatorCornerRadius" format="dimension"/>
    <attr name="acl_overlineColor" format="color"/>
    <attr name="acl_overlineThickness" format="dimension"/>
    <attr name="acl_underlineColor" format="color"/>
    <attr name="acl_underlineThickness" format="dimension"/>
    <attr name="acl_dividerColor" format="color"/>
    <attr name="acl_dividerColors" format="reference"/>
    <attr name="acl_dividerThickness" format="dimension"/>
    <attr name="acl_defaultTabBackground" format="reference"/>
    <attr name="acl_defaultTabTextAllCaps" format="boolean"/>
    <attr name="acl_defaultTabTextColor" format="color|reference"/>
    <attr name="acl_defaultTabTextSize" format="dimension"/>
    <attr name="acl_defaultTabTextHorizontalPadding" format="dimension"/>
    <attr name="acl_defaultTabTextMinWidth" format="dimension"/>
    <attr name="acl_customTabTextLayoutId" format="reference"/>
    <attr name="acl_customTabTextViewId" format="reference"/>
    <attr name="acl_distributeEvenly" format="boolean"/>
    <attr name="acl_clickable" format="boolean"/>
    <attr name="acl_titleOffset" format="dimension">
      <enum name="auto_center" value="-1"/>
    </attr>
    <attr name="acl_drawDecorationAfterTab" format="boolean"/>
  </declare-styleable>
</resources>
